<div id="{{_id}}" class="image-gallery-component">
	<div class="gallery">
		{{#each images}}
			<div class="gallery-image" >
				<div class="blur-back" style="background-image: url({{this}})"></div>
				<img src="{{this}}"  alt="">
			</div>
		{{/each}}

		<span class="prev">&#10094;</span>
		<span class="next">&#10095;</span>

		<div class="thumbnail-row">
			{{#each images}}
				<div class="thumbnail-column">
					<img class="thumbnail" src="{{this}}" alt="">
				</div>
			{{/each}}
		</div>
	</div>
</div>
